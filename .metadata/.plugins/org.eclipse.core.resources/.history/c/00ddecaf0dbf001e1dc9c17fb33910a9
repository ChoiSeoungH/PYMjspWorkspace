package basic;

public class Util {
	
	boolean minus = false;
	public boolean isNumValue(String str) {
		if(str.charAt(0) == '-') {
			minus = true;
		}else if(Character.isDigit(str.charAt(0))) {
			return false;
		}
		
		for(int i=1; i<str.length(); i++) {
			if(!Character.isDigit(str.charAt(i))) {
				return false;
			}
		}
		return true;
	}
}
